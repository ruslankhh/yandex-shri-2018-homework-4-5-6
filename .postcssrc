{
  "map": true,
  "parser": "postcss-scss",
  "plugins": {
    "postcss-import": {
      "path": "./client/src"
    },
    "postcss-mixins": {},
    "postcss-simple-vars": {},
    "postcss-nested": {},
    "postcss-svg": {
      "paths": [
        "./client/src/static/images"
      ]
    },
    "postcss-assets": {
      "basePath": "./client/src/static",
      "loadPaths": [
        "./client/src/static"
      ],
      "cachebuster": true,
      "cache": true
    },
    "precss": {},
    "autoprefixer": {
      "cascade": true
    }
  }
}
